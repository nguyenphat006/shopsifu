{
  "name": "shopsifu",
  "version": "1.0.0",
  "description": "Shopsifu",
  "author": "TedyDev",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' npx @nestjs/cli build",
    "start": "node dist/main",
    "dev": "npx @nestjs/cli start --watch",
    "debug": "npx @nestjs/cli start --debug --watch",
    "update:package": "npx npm-check-updates -u",
    "clean:package": "rm -rf node_modules && sleep 5 && npm install",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "studio": "prisma studio",
    "seed:email": "nestjs-command seed:emails",
    "seed": "ts-node -r tsconfig-paths/register initialScript/init/init-data.ts && ts-node -r tsconfig-paths/register initialScript/init/create-permissions.ts && ts-node -r tsconfig-paths/register initialScript/init/create-languages.ts && ts-node -r tsconfig-paths/register initialScript/product/script/import-main.ts",
    "seed:with-sync": "ts-node -r tsconfig-paths/register initialScript/product/script/seed-with-sync.ts",
    "init-seed-data": "ts-node -r tsconfig-paths/register initialScript/init/init-data.ts",
    "create-languages": "ts-node -r tsconfig-paths/register initialScript/init/create-languages.ts",
    "create-permissions": "ts-node -r tsconfig-paths/register initialScript/init/create-permissions.ts",
    "clear-products": "ts-node -r tsconfig-paths/register initialScript/product/script/clear-products.ts",
    "sync-es": "ts-node -r tsconfig-paths/register initialScript/product/script/sync-to-elasticsearch.ts",
    "import:reset": "npm run clear-products && npm run import-products && npm run sync-to-elasticsearch",
    "db:reset": "npx prisma migrate reset --force && npm run seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.json --runInBand",
    "prepare": "husky",
    "test:comprehensive": "ts-node src/scripts/test-comprehensive.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/lib-storage": "^3.856.0",
    "@aws-sdk/s3-request-presigner": "^3.856.0",
    "@elastic/elasticsearch": "^8.19.0",
    "@keyv/redis": "^5.0.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.5",
    "@prisma/client": "^6.11.1",
    "@react-email/components": "^0.3.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/ioredis": "^4.28.10",
    "@types/moment": "^2.13.0",
    "@types/qs": "^6.14.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.8",
    "cache-manager": "^7.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "giaohangnhanh": "^0.0.5",
    "googleapis": "^154.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.5.1",
    "nestjs-pino": "^4.4.0",
    "nestjs-vnpay": "^1.0.4",
    "nestjs-zod": "^4.3.1",
    "otpauth": "^9.4.0",
    "pino-pretty": "^13.1.1",
    "qs": "^6.14.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "redis": "^5.6.1",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.7.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "vnpay": "^2.4.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^11.0.8",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "prisma-json-types-generator": "^3.5.1",
    "react-email": "^4.2.5",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
